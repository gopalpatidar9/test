<% if @artical.errors.any? %>
 <h1> not correct chake </h1>
 <ul>
  <% @artical.errors.full_messages.each do |msg|%>
    <li><%= msg %></li>
  <% end %>
 </ul>
<% end %>

<%= form_with(model: @artical , local: true ) do |f| %>
<p>
  <%= f.label :title %><br>
  <%= f.text_field :title%>
</p>

<p>
  <%= f.label :description %><br>
  <%= f.text_area :discription%>
</p>

<p>
  <%= f.submit%>
</p>
<% end %>
